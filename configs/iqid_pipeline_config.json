{
  "pipeline_name": "iQID_Only_Processing",
  "version": "1.0",
  "description": "Configuration for iQID-only processing pipeline (ReUpload data)",
  "path_config_file": "./configs/path_config.json",
  "paths": {
    "data_source": "reupload_directory",
    "output_directory": "iqid_only_processing",
    "temp_directory": "temp_processing",
    "logs_directory": "logs_directory"
  },
  "processing_parameters": {
    "segmentation": {
      "method": "morphological",
      "min_size": 50,
      "watershed_markers": "auto"
    },
    "alignment": {
      "method": "boundary_based",
      "alignment_type": "centroid_alignment"
    }
  },
  "quality_thresholds": {
    "min_roi_size": 50,
    "max_rois_per_sample": 100
  },
  "output_settings": {
    "save_intermediate_results": true,
    "create_visualizations": true,
    "generate_reports": true
  }
}
