{
  "pipeline_name": "iQID_Only_Processing",
  "version": "1.0",
  "description": "Configuration for iQID-only processing pipeline (ReUpload data)",
  "paths": {
    "reupload_root": "../data/UCSF-Collab/data/ReUpload",
    "output_root": "./outputs/iqid_only_processing"
  },
  "processing_parameters": {
    "segmentation": {
      "method": "morphological",
      "min_size": 50,
      "watershed_markers": "auto"
    },
    "alignment": {
      "method": "boundary_based",
      "alignment_type": "centroid_alignment"
    }
  },
  "quality_thresholds": {
    "min_roi_size": 50,
    "max_rois_per_sample": 100
  }
}
